{
    "display": "Node LTS",
    "_version": "20.1.0",
    "compilerOptions": {
      "lib": [
        "es2023"
      ],
      "module": "node16",
      "target": "es2022",
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "noUncheckedIndexedAccess": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node16"
    },
    "include": [
      "src/**/*.ts"
    ],
  }